{"operationName":"GetSuggestedPeople","variables":{"country":"FR","language":"fr","first":4,"filter":{"searchQuery":"Good Omen"}},"query":"query GetSuggestedPeople($country: Country!, $language: Language!, $first: Int!, $filter: PersonFilter) {\n  popularPeople(country: $country, first: $first, filter: $filter) {\n    edges {\n      node {\n        ...SuggestedPerson\n        __typename\n      }\n      __typename\n    }\n    __typename\n  }\n}\n\nfragment SuggestedPerson on Person {\n  id\n  objectType\n  objectId\n  content(country: $country, language: $language) {\n    fullName\n    dateOfBirth\n    __typename\n  }\n  __typename\n}\n"}
